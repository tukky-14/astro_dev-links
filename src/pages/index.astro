---
import Layout from '../layouts/Layout.astro';
import ShareIcon from '@mui/icons-material/Share';
---

<Layout title="Welcome to Astro.">
    <header class="bg-gray-400 mb-10">
        <div class="flex items-center py-3 px-4 text-xl">
            <ShareIcon className="h-5" />
            <span class="ml-1">Dev Links</span>
        </div>
    </header>
    <main>
        <div class="mx-6 grid grid-cols-3 gap-6">
            <div class="h-40 bg-white"></div>
            <div class="h-40 bg-white"></div>
            <div class="h-40 bg-white"></div>
        </div>
    </main>
</Layout>
